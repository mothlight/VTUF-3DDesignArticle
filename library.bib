@article{Southworth2005,
abstract = {With federal policy beginning to shift from autocentric planning, provision for pedestrian and bicycle access is now mandated in federally supported projects. However, the field of transportation planning has little in the way of theory and methods to guide design and planning for walkable cities. Walkability is increasingly valued for a variety of reasons. Not only does pedestrian transportation reduce congestion and have low environmental impact, it has social and recreational value. Recent research suggests that walking also promotes mental and physical health. The quality of the pedestrian environment is key to encouraging people to choose walking over driving. Six criteria are presented for design of a successful pedestrian network: ͑1͒ connectivity; ͑2͒ linkage with other modes; ͑3͒ fine grained land use patterns; ͑4͒ safety; ͑5͒ quality of path; and ͑6͒ path context. To achieve walkable cities in the United States it will be necessary to assess current walkability conditions, revise standards and regulations, research walking behavior in varied settings, promote public education and participation in pedestrian planning, and encourage collaboration and interdisciplinary education between transportation engineers and the design professions.},
author = {Southworth, Michael},
doi = {10.1061/(ASCE)0733-9488(2005)131:4(246)},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Southworth - 2005 - Designing the Walkable City - Journal of Urban Planning and Development.pdf:pdf},
isbn = {0733-9488()131:4L.246;1},
issn = {0733-9488},
journal = {Journal of Urban Planning and Development},
number = {December},
pages = {246--257},
pmid = {18861206},
title = {{Designing the Walkable City}},
volume = {December},
year = {2005}
}
