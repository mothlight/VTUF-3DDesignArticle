@article{Loureiro2014,
abstract = {The way adolescents travel to school can be an important health promotion strategy. The main objective of this research was to identify the relationships between the Portuguese adolescents? perception of their neighbourhood and their option for active transportation (AT). The Health Behaviour in School-aged Children 2010 questionnaire, was applied to 3,494 adolescents with an average age of 14.93 (DP?±?1.29), 53.6{\%} of whom were female. The associations were studied by applying ?² tests and multivariate logistic regression models. In this study, 35{\%} of the sample reported walking or cycling to school. AT to school is associated with age, more frequent in the adolescents aged 16 and older (OR?=?1.19, p?{\textless}?.05), more frequent in residential areas with good public services (OR?=?1.79, p?{\textless}?.05), as well as in beautiful neighbourhoods (OR?=?1.76, p?{\textless}?.05), less frequent in neighbourhoods with nightlife entertainment (OR?=?0.73, p?{\textless}?.05), and in those characterized by violence and theft (OR?=?0.56, p?{\textless}?.05) as well as in isolated areas (OR?=?0.66, p?{\textless}?.05). Finally, it was more frequent when the travel time from home to school was lower (OR?=?0.57, p?{\textless}?.05). Conclusion: The results show that the adolescents? perceptions about some of the characteristics of their neighbourhood environments are associated with their options concerning AT when travelling to school.$\backslash$nThe way adolescents travel to school can be an important health promotion strategy. The main objective of this research was to identify the relationships between the Portuguese adolescents? perception of their neighbourhood and their option for active transportation (AT). The Health Behaviour in School-aged Children 2010 questionnaire, was applied to 3,494 adolescents with an average age of 14.93 (DP?±?1.29), 53.6{\%} of whom were female. The associations were studied by applying ?² tests and multivariate logistic regression models. In this study, 35{\%} of the sample reported walking or cycling to school. AT to school is associated with age, more frequent in the adolescents aged 16 and older (OR?=?1.19, p?{\textless}?.05), more frequent in residential areas with good public services (OR?=?1.79, p?{\textless}?.05), as well as in beautiful neighbourhoods (OR?=?1.76, p?{\textless}?.05), less frequent in neighbourhoods with nightlife entertainment (OR?=?0.73, p?{\textless}?.05), and in those characterized by violence and theft (OR?=?0.56, p?{\textless}?.05) as well as in isolated areas (OR?=?0.66, p?{\textless}?.05). Finally, it was more frequent when the travel time from home to school was lower (OR?=?0.57, p?{\textless}?.05). Conclusion: The results show that the adolescents? perceptions about some of the characteristics of their neighbourhood environments are associated with their options concerning AT when travelling to school.},
author = {Loureiro, Nuno and {Gaspar de Matos}, Margarida},
doi = {10.1080/21650020.2014.922896},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Cycling/Loureiro, Gaspar de Matos - 2014 - Why don't they walk or cycle Reflections on active home–school transportation among Portuguese ad.pdf:pdf},
journal = {Urban, Planning and Transport Research},
number = {August},
pages = {265--273},
title = {{Why don't they walk or cycle? Reflections on active home–school transportation among Portuguese adolescents: the role of environmental perceptions}},
url = {http://www.tandfonline.com/doi/abs/10.1080/21650020.2014.922896},
year = {2014}
}
