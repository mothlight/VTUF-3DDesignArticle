@article{Krayenhoff2007b,
abstract = {A microscale three-dimensional (3-D) urban energy balance model, Temperatures$\backslash$nof Urban Facets in 3-D (TUF-3D), is developed to predict urban surface$\backslash$ntemperatures for a variety of surface geometries and properties,$\backslash$nweather conditions, and solar angles. The surface is composed of$\backslash$nplane-parallel facets: roofs, walls, and streets, which are further$\backslash$nsub-divided into identical square patches, resulting in a 3-D raster-type$\backslash$nmodel geometry. The model code is structured into radiation, conduction$\backslash$nand convection sub-models. The radiation sub-model uses the radiosity$\backslash$napproach and accounts for multiple reflections and shading of direct$\backslash$nsolar radiation. Conduction is solved by finite differencing of the$\backslash$nheat conduction equation, and convection is modelled by empirically$\backslash$nrelating patch heat transfer coefficients to the momentum forcing$\backslash$nand the building morphology. The radiation and conduction sub-models$\backslash$nare tested individually against measurements, and the complete model$\backslash$nis tested against full-scale urban surface temperature and energy$\backslash$nbalance observations. Modelled surface temperatures perform well$\backslash$nat both the facet-average and the sub-facet scales given the precision$\backslash$nof the observations and the uncertainties in the model inputs. The$\backslash$nmodel has several potential applications, such as the calculation$\backslash$nof radiative loads, and the investigation of effective thermal anisotropy$\backslash$n(when combined with a sensor-view model).},
author = {Krayenhoff, E. Scott and Voogt, James a.},
doi = {10.1007/s10546-006-9153-6},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Downloaded-disable/Krayenhoff, Voogt - 2007 - A microscale three-dimensional urban energy balance model for studying surface temperatures.pdf:pdf},
isbn = {0006-8314},
issn = {0006-8314},
journal = {Boundary-Layer Meteorology},
keywords = {b,e,krayenhoff,microclimate,model validation,radiative exchange,s,surface,temperature,thermal remote sensing,urban energy balance model},
number = {3},
pages = {433--461},
title = {{A microscale three-dimensional urban energy balance model for studying surface temperatures}},
volume = {123},
year = {2007}
}
@techreport{Gebel2009,
abstract = {Physical inactivity is a modifi able risk factor for cardiovascular disease and a range of other chronic diseases, including diabetes mellitus, cancer (colon and breast), obesity, hypertension, bone and joint diseases and depression.1–3 • Physical activity is important in reducing risk factors for cardiovascular and other chronic diseases.4,5 • Promoting walking is recognised as a promising means of increasing population levels of physical activity. • The built environment is directly associated with physical activity, particularly walking. • The built environment can either facilitate or discourage walking. • Walking for transport is associated with living in neighbourhoods that have good access to destinations (including public transport), connected street networks, and higher residential densities.6,7 • Neighbourhood aesthetics (including access to public open space) tend to be associated with increased walking for recreation. • There is widespread agreement that there is suffi cient evidence to warrant public health action on the role of the built environment in increasing physical activity.6–9 • A whole-of-government approach is crucial to the creation of ‘walkable' communities in new and existing developments.},
author = {Gebel, Klaus and Bauman, Adrian and Owen, Neville and Foster, Sarah and Giles-corti, Billie},
booktitle = {National Heart Foundation of Australia},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Gebel et al. - 2009 - Position statement The built environment and walking - National Heart Foundation of Australia.pdf:pdf},
pages = {1--16},
title = {{Position statement The built environment and walking}},
year = {2009}
}
