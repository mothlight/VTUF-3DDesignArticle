@article{Lee2011a,
abstract = {The performance of different urban surface parameterizations in the WRF (Weather Research and Forecasting) in simulating urban boundary layer (UBL) was investigated using extensive measurements during the Texas Air Quality Study 2006 field campaign. The extensive field measurements collected on surface (meteorological, wind profiler, energy balance flux) sites, a research aircraft, and a research vessel characterized 3-dimensional atmospheric boundary layer structures over the Houston-Galveston Bay area, providing a unique opportunity for the evaluation of the physical parameterizations. The model simulations were performed over the Houston metropolitan area for a summertime period (12-17 August) using a bulk urban parameterization in the$\backslash$nNoah land surface model (original LSM), a modified LSM, and a single-layer$\backslash$nurban canopy model (UCM). The UCM simulation compared quite well$\backslash$nwith the observations over the Houston urban areas, reducing the$\backslash$nsystematic model biases in the original LSM simulation by 1-2 degrees$\backslash$nC in near-surface air temperature and by 200-400 m in UBL height,$\backslash$non average. A more realistic turbulent (sensible and latent heat)$\backslash$nenergy partitioning contributed to the improvements in the UCM simulation.$\backslash$nThe original LSM significantly overestimated the sensible heat flux$\backslash$n(similar to 200 W m(-2)) over the urban areas, resulting in warmer$\backslash$nand higher UBL. The modified LSM slightly reduced warm and high biases$\backslash$nin near-surface air temperature (0.5-1 degrees C) and UBL height$\backslash$n(similar to 100 m) as a result of the effects of urban vegetation.$\backslash$nThe relatively strong thermal contrast between the Houston area and$\backslash$nthe water bodies (Galveston Bay and the Gulf of Mexico) in the LSM$\backslash$nsimulations enhanced the sea/bay breezes, but the model performance$\backslash$nin predicting local wind fields was similar among the simulations$\backslash$nin terms of statistical evaluations. These results suggest that a$\backslash$nproper surface representation (e.g. urban vegetation, surface morphology)$\backslash$nand explicit parameterizations of urban physical processes are required$\backslash$nfor accurate urban atmospheric numerical modeling.},
author = {Lee, S. H. and Kim, S. W. and Angevine, W. M. and Bianco, L. and McKeen, S. A. and Senff, C. J. and Trainer, M. and Tucker, S. C. and Zamora, R. J.},
doi = {10.5194/acp-11-2127-2011},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Lee et al. - 2011 - Evaluation of urban surface parameterizations in the WRF model using measurements during the Texas Air Quality Study.pdf:pdf},
issn = {16807316},
journal = {Atmospheric Chemistry and Physics},
number = {5},
pages = {2127--2143},
title = {{Evaluation of urban surface parameterizations in the WRF model using measurements during the Texas Air Quality Study 2006 field campaign}},
volume = {11},
year = {2011}
}
