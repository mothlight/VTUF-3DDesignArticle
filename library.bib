@article{Jarvi2014R,
abstract = {The Surface Urban Energy and Water Balance Scheme (SUEWS) is developed$\backslash$nto include snow. The processes addressed include accumulation of snow on$\backslash$nthe different urban surface types: snow albedo and density aging, snow$\backslash$nmelting and re-freezing of meltwater. Individual model parameters are$\backslash$nassessed and independently evaluated using long-term observations in the$\backslash$ntwo cold climate cities of Helsinki and Montreal. Eddy covariance$\backslash$nsensible and latent heat fluxes and snow depth observations are$\backslash$navailable for two sites in Montreal and one in Helsinki. Surface runoff$\backslash$nfrom two catchments (24 and 45 ha) in Helsinki and snow properties$\backslash$n(albedo and density) from two sites in Montreal are also analysed. As$\backslash$nmultiple observation sites with different land-cover characteristics are$\backslash$navailable in both cities, model development is conducted independent of$\backslash$nevaluation. The developed model simulates snowmelt related runoff well$\backslash$n(within 19{\%} and 3{\%} for the two catchments in Helsinki when there is$\backslash$nsnow on the ground), with the springtime peak estimated correctly.$\backslash$nHowever, the observed runoff peaks tend to be smoother than the$\backslash$nsimulated ones, likely due to the water holding capacity of the$\backslash$ncatchments and the missing time lag between the catchment and the$\backslash$nobservation point in the model. For all three sites the model simulates$\backslash$nthe timing of the snow accumulation and melt events well, but$\backslash$nunderestimates the total snow depth by 18-20{\%} in Helsinki and 29-33{\%}$\backslash$nin Montreal. The model is able to reproduce the diurnal pattern of net$\backslash$nradiation and turbulent fluxes of sensible and latent heat during cold$\backslash$nsnow, melting snow and snow-free periods. The largest model$\backslash$nuncertainties are related to the timing of the melting period and the$\backslash$nparameterization of the snowmelt. The results show that the enhanced$\backslash$nmodel can simulate correctly the exchange of energy and water in cold$\backslash$nclimate cities at sites with varying surface cover.},
author = {J{\"{a}}rvi, L. and Grimmond, C. S B and Taka, M. and Nordbo, A. and Set{\"{a}}l{\"{a}}, H. and Strachan, I. B.},
doi = {10.5194/gmd-7-1691-2014},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/J{\"{a}}rvi et al. - 2014 - REVIEWS-Development of the Surface Urban Energy and Water Balance Scheme (SUEWS) for cold climate cities - Geosci.pdf:pdf},
issn = {19919603},
journal = {Geoscientific Model Development},
number = {4},
pages = {1691--1711},
title = {{REVIEWS-Development of the Surface Urban Energy and Water Balance Scheme (SUEWS) for cold climate cities}},
volume = {7},
year = {2014}
}
