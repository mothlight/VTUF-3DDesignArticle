keywords = {Green spaces,Nature,Noise annoyance,Noise perception,Noise sensitivity},
number = {4},
pages = {1000--1008},
publisher = {Elsevier GmbH.},
title = {{Green spaces and environmental noise perception}},
url = {http://dx.doi.org/10.1016/j.ufug.2015.09.006},
volume = {14},
year = {2015}
}
