@incollection{Elmer2012,
abstract = {Global warming has given a new dimension to urban design efforts that$\backslash$nseek to integrate the infrastructure systems of a city into a$\backslash$nsustainable and more natural built environment. The result has been a$\backslash$ntransformation in approaches taken by leading edge design practitioners$\backslash$nand the elites responsible for urban infrastructure systems. Many group$\backslash$nthese responses under the rubric of the `eco-city.' Nowhere has this$\backslash$nbeen more evident than in the water and energy sectors. Mitigating$\backslash$ncarbon emissions has been taken up by local governments around the$\backslash$nworld, while water is the most visible vector for the effects of global$\backslash$nwarming.$\backslash$nThis paper first describes innovations in energy, solid waste and$\backslash$nwastewater at the building level before examining how they are combined$\backslash$nin six European neighbourhoods as an integrated `micro utility,' taking$\backslash$nparticular advantage of the public and private landscape. We conclude$\backslash$nthat greater integration of waste systems and the urban landscape is the$\backslash$nfinal secret to achieving `zero emissions' operation. The neighbourhood$\backslash$nis the scale that makes more sustainable utility systems possible and$\backslash$nthe landscape is both the technical underpinning and the aesthetic place$\backslash$nmaking element, becoming the fifth infrastructure.},
author = {Elmer, Vicki and Fraker, Harrison},
booktitle = {Water Sensitive Cities},
chapter = {9},
editor = {Howe, Carol and Mitchell, Cynthia},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Elmer, Fraker - 2011 - Water, neighborhoods and urban design micro-utilities and the fifth infrastructure - Water Sensitive Cities.pdf:pdf},
isbn = {978-1-84339-364-1},
keywords = {Integrated water management,eco-city,landscape},
pages = {193--207},
publisher = {IWA Publishing,},
title = {{Water, neighborhoods and urban design: micro-utilities and the fifth infrastructure}},
year = {2011}
}
@article{Makela2000,
abstract = {Recent progress toward the application of pro- cess-based models in forest management includes the develop- ment of evaluation and parameter estimation methods suitable for models with causal structure, and the accumulation of data that can be used in model evaluation.Thecurrent state of the art of process modeling is discussed in the context of forest eco- system management. We argue that the carbon balance ap- proach is readily applicable for projecting forest yield and productivity, and review several carbon balance models for es- timating stand productivity and individual tree growth and competition.Wepropose that to develop operational models, it is necessary to accept that all models may have both empirical and causal components at the system level. We present exam- ples of hybrid carbon balance models and consider issues that currently require incorporation of empirical information at the system level. We review model calibration and validation methods that take account of the hybrid character of models. The operational implementation of process-based models to practical forest management is discussed. Methods of deci- sion-making in forest management are gradually moving to- ward a more general, analytical approach, and it seems likely that models that include some process-oriented components will soon be used in forestry enterprises. This development is likely to run parallel with the further development of ecophysiologically based models.},
author = {M{\"{a}}kel{\"{a}}, Annikki and Landsberg, Joe O E and Ek, Alan R and Burk, Thomas E and Ter-Mikaelian, Michael and {\AA}gren, G{\"{o}}ran I and Oliver, Chadwick D and Puttonen, Pasi},
doi = {10.1093/treephys/20.5-6.289},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/M{\"{a}}kel{\"{a}} et al. - 2000 - Process-based models for forest ecosystem management current state of the art and challenges for practical imp.pdf:pdf},
isbn = {0829-318X},
issn = {1758-4469},
journal = {Tree Physiology},
keywords = {carbon allocation,carbon balance,competition,stand productivity,tree growth},
pages = {289--298},
pmid = {12651445},
title = {{Process-based models for forest ecosystem management : current state of the art and challenges for practical implementation}},
volume = {20},
year = {2000}
}
