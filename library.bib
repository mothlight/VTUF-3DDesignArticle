file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Monash Masters/Semester 2/Semester 2-Anu/Moss et al. - 2010 - The next generation of scenarios for climate change research and assessment. - Nature.pdf:pdf},
issn = {1476-4687},
journal = {Nature},
keywords = {Ecology,Ecology: trends,Global Warming,Global Warming: prevention {\&} control,Global Warming: statistics {\&} numerical data,Human Activities,Risk Assessment,Vehicle Emissions},
month = {feb},
number = {7282},
