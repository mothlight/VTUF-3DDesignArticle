journal = {Journal of Urban Planning and Development},
keywords = {Morphological spatial pattern analysis,Temperature regulation,Urban green connectivity,Urban heat islands},
number = {3},
pages = {A5014008},
title = {{Dynamics and Temperature Regulation Function of Urban Green Connectivity}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84939129609{\&}partnerID=tZOtx3y1},
volume = {141},
year = {2015}
}
