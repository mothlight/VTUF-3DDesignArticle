@article{Lenzholzer2016,
abstract = {'Research Through Designing' (RTD) is a research method that is based on the active employment of designing in the research process. Often, RTD is necessary to generate knowledge that is relevant for design such as design guidelines or prototypes. A broad range of RTD methods can be used to produce such results: post-positivist, constructivist, participatory and pragmatist approaches. The aim of this paper is to elucidate the post-positivist RTD methods through the discussion of examples. The examples represent microclimate responsive design research and were derived from an extensive literature review. The typical issues to be dealt with in such studies are discussed: complexity, scale, testing methods and their mutual relations. A distinction is made between RTD methods and other design research for microclimatic improvement. Three studies occurred to be RTD in the literal sense and they provide a methodological model for further research to generate evidence that supports urban microclimate responsive design.},
author = {Lenzholzer, Sanda and Brown, Robert D.},
doi = {10.1016/j.landurbplan.2016.05.008},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/81-s2.0-S0169204616300743-main.pdf:pdf},
issn = {01692046},
journal = {Landscape and Urban Planning},
keywords = {Landscape architecture,Methodology,Microclimate,Research through designing,Urban design},
pages = {111--121},
publisher = {Elsevier B.V.},
title = {{Post-positivist microclimatic urban design research: A review}},
url = {http://dx.doi.org/10.1016/j.landurbplan.2016.05.008},
volume = {153},
year = {2016}
}
