@inproceedings{Koerniawan2014,
abstract = {Nowadays, climatology, thermal comfort, and walkability have already become the main issues of some researches in an urban context in the term to make urban area to be easier to live in. The people' awareness of open space and green area is emergent as well in order to provide opportunities for people interaction, physical exercise and leisure especially for the children and the elderly. One proposed definition for a better place in the open space is the extent to which outdoor space is friendly to the presence of people activity. In the tropical hot-humid climate, outdoor spaces are used during the year, and they must provide proper levels of thermal comfort. This study was carried out in Jakarta (Indonesia) with the aim of discovering, through these indices, the natural and ecological effects of urban design which can improve thermal comfort and walkability. The result shows thermal comfort is difficult to achieve in the open space among buildings of superblock in Jakarta. The heat trapped among the buildings in the night time affect the temperature in the morning made Tmrt (radiation) raise significantly in the day. Tmrt did not only affect the thermal comfort in open space but also affect the walkability. The simulation result that the people mileage of walking is affected by Tmrt. Shadowing is needed in the term of Thermal Comfort and Walkability in open space of Superblock.},
author = {Koerniawan, M Donny and Gao, Weijun},
doi = {10.13140/2.1.4388.5766},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Koerniawan, Gao - 2014 - Thermal Comfort and Walkability In Open Spaces of Mega Kuningan Superblock in Jakarta - Unknown.pdf:pdf},
title = {{Thermal Comfort and Walkability In Open Spaces of Mega Kuningan Superblock in Jakarta}},
year = {2014}
}
