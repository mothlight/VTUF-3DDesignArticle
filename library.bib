keywords = {MAESTRA,eucalypt,eucalyptus,metamodeling,mod{\`{e}}le de substitution,mod{\`{e}}le fonctionnel,m{\'{e}}ta-mod{\'{e}}lisation,process-based model,surrogate model},
number = {January},
pages = {297--309},
title = {{Sensitivity and uncertainty analysis of the carbon and water fluxes at the tree scale in Eucalyptus plantations using a metamodeling approach 1}},
url = {http://www.nrcresearchpress.com/doi/full/10.1139/cjfr-2015-0173{\#}.VrzKdlKGemh},
