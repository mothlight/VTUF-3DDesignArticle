@article{Zhao2013,
abstract = {Actual evapotranspiration is a key process of hydrological cycle and a sole term that links land surface water balance and land surface energy balance. Evapotranspiration plays a key role in simulating hydrological effect of climate change, and a review of evapotranspira- tion estimation methods in hydrological models is of vital importance. This paper firstly summarizes the evapotranspiration estimation methods applied in hydrological models and then classifies them into the integrated converting methods and the classification gathering methods by their mechanism. Integrated converting methods are usually used in hydrological models and two differences exist among them: one is in the potential evaporation estimation methods, while the other in the function for defining relationship between potential evapora- tion and actual evapotranspiration. Due to the higher information requirements of the Pen- man-Monteith method and the existing data uncertainty, simplified empirical methods for calculating potential and actual evapotranspiration are widely used in hydrological models. Different evapotranspiration calculation methods are used depending on the complexity of the hydrological model, and importance and difficulty in the selection of the most suitable evapotranspiration methods is discussed. Finally, this paper points out the prospective de- velopment trends of the evapotranspiration estimating methods in hydrological modeling.},
author = {Zhao, Lingling and Xia, Jun and yu Xu, Chong and Wang, Zhonggen and Sobkowiak, Leszek and Long, Cangrui},
doi = {10.1007/s11442-013-1015-9},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Zhao et al. - 2013 - Evapotranspiration estimation methods in hydrological models - Journal of Geographical Sciences.pdf:pdf},
isbn = {1009-637X},
issn = {1009637X},
journal = {Journal of Geographical Sciences},
keywords = {actual evaporation,function of soil moisture,hydrological model,potential evaporation},
number = {2},
pages = {359--369},
title = {{Evapotranspiration estimation methods in hydrological models}},
volume = {23},
year = {2013}
}
