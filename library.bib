doi = {10.1139/cjfr-2015-0173},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Christina et al. - 2016 - Sensitivity and uncertainty analysis of the carbon and water fluxes at the tree scale in Eucalyptus plantation.pdf:pdf},
