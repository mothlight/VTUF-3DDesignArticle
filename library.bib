issn = {0733-9488},
journal = {Journal of Urban Planning and Development},
number = {0},
pages = {A4015001},
title = {{Brownfields As an Element of Green Infrastructure for Implementing Ecosystem Services into Urban Areas}},
url = {http://dx.doi.org/10.1061/(ASCE)UP.1943-5444.0000275{\%}5Cnhttp://ascelibrary.org/doi/abs/10.1061/(ASCE)UP.1943-5444.0000275{\%}5Cnhttp://ascelibrary.org/doi/pdf/10.1061/(ASCE)UP.1943-5444.0000275},
