journal = {Indoor air},
keywords = {Body Temperature Regulation,Cold Temperature,Forecasting,Human Engineering,Humans,Models, Theoretical,Reference Values,Skin Temperature,Thermosensing,Volatilization,Water-Electrolyte Balance},
month = {jan},
