@article{Thorsson2014,
abstract = {Health studies have repeatedly used air temperature (. T{\textless}inf{\textgreater}a{\textless}/inf{\textgreater}), sometimes adjusted for humidity, when analyzing the impact of weather on mortality. The aim of this study is to highlight the importance of mean radiant temperature (. T{\textless}inf{\textgreater}mrt{\textless}/inf{\textgreater}) and its impact on heat related mortality. T{\textless}inf{\textgreater}mrt{\textless}/inf{\textgreater} is an essential meteorological parameter that influences the thermal comfort (heat load) of humans. It is useful when assessing the impact of weather, especially heat, on people's health. T{\textless}inf{\textgreater}mrt{\textless}/inf{\textgreater} is directly influenced by urban geometry and surface material, which also makes it a good measure to identify urban hot spots. The performance of models using T{\textless}inf{\textgreater}a{\textless}/inf{\textgreater} and T{\textless}inf{\textgreater}mrt{\textless}/inf{\textgreater} for daily mortality is compared for Stockholm County, Sweden. It is demonstrated that T{\textless}inf{\textgreater}mrt{\textless}/inf{\textgreater} models fit heat related mortality better than T{\textless}inf{\textgreater}a{\textless}/inf{\textgreater} models, which implies that health studies should consider using T{\textless}inf{\textgreater}mrt{\textless}/inf{\textgreater} rather than T{\textless}inf{\textgreater}a{\textless}/inf{\textgreater}. The use of T{\textless}inf{\textgreater}mrt{\textless}/inf{\textgreater} models allows us to determine more accurate thresholds for increased risks of heat related mortality, and thus to better identify adverse weather conditions and heat prone urban geometries. Such information is needed to implement heat-warning systems and mitigate harmful effects of heat stress.},
author = {Thorsson, Sofia and Rocklov, Joacim and Konarska, Janina and Lindberg, Fredrik and Holmer, Bjorn and Dousset, B??n??dicte and Rayner, David},
doi = {10.1016/j.uclim.2014.01.004},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Thorsson et al. - 2014 - Mean radiant temperature - A predictor of heat related mortality - Urban Climate.pdf:pdf},
isbn = {2212-0955},
issn = {22120955},
journal = {Urban Climate},
keywords = {Heat related mortality,Heat stress,Hot spots,Mean radiant temperature},
number = {P2},
pages = {332--345},
title = {{Mean radiant temperature - A predictor of heat related mortality}},
volume = {10},
year = {2014}
}
