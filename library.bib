@article{Bodin2013,
abstract = {Accurate modelling of long-term changes in plant stomatal functioning is vital to global climate change studies because changes in evapotranspiration influence temperature via physiological forcing of the climate. Various stomatal models are included in land surface schemes, but their robustness over longer timescales is difficult to validate. We compare the performance of three stomatal models, varying in their degree of complexity, and coupled to a land surface model. This is carried out by simulating the carbon isotope ratio of tree leaves ($\delta$13Cleaf) over a period of 53 years, and comparing the results with carbon isotope ratios obtained from tree rings ($\delta$13Cstem) measured at six sites in northern Europe. All three stomatal models fail to capture the observed interannual variability in the measured $\delta$13Cstem time series. However, the Soil-Plant-Atmosphere (SPA) model performs significantly better than the Ball-Berry (BB) or COX models when tested for goodness-of-fit against measured $\delta$13Cstem. The $\delta$13Cleaf time series simulated using the SPA model are significantly positively correlated (P {\textless} 0.05) with measured results over the full time period tested, at all six sites. The SPA model underestimates interannual variability measured in $\delta$13Cstem, but is no worse than the BB model and significantly better than the COX model. The inability of current models to adequately replicate changes in stomatal response to rising levels of CO2 concentrations, and thus to quantify the associated physiological forcing, warrants further investigation.},
author = {Bodin, P. E. and Gagen, Mary and McCarroll, Danny and Loader, Neil J. and Jalkanen, Risto and Robertson, Iain and Switsur, Vincent R. and Waterhouse, John S. and Woodley, Ewan J. and Young, Giles H F and Alton, Paul B.},
doi = {10.1111/gcb.12192},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Bodin et al. - 2013 - Comparing the performance of different stomatal conductance models using modelled and measured plant carbon isoto.pdf:pdf},
isbn = {1354-1013 (Print)$\backslash$n1354-1013 (Linking)},
issn = {13541013},
journal = {Global Change Biology},
keywords = {Ball-Berry,Dendrochronology,Internal conductance,Land surface model,SPA model,Tree-rings},
number = {6},
pages = {1709--1719},
pmid = {23504999},
title = {{Comparing the performance of different stomatal conductance models using modelled and measured plant carbon isotope ratios ($\delta$13C): Implications for assessing physiological forcing}},
volume = {19},
year = {2013}
}
