@article{Salata2015,
abstract = {Outdoor thermal comfort affects the health of the people and the quality of life in urban areas. This is the reason why in the past few years different mitigation strategies for the microclimate have been studied and examined. These strategies depend on those passive factors characterizing the urban setting that are able to affect the values of local meteorological variables, as the limit surfaces of the urban space (parterre and fa{\c{c}}ades of the buildings). This paper examines the Cloister by Giuliano da Sangallo, which is part of the Faculty of Engineering of Sapienza University of Rome. The case study compares the present configuration of the Cloister with four other configurations characterized by some vegetation and materials with a high albedo by taking into consideration the PMV (Predicted Mean Vote) model. Microclimatic variables are calculated through numerical simulations performed by the ENVI-met software. Such a comparison is performed during a typical summer day. While examining the results it can be noticed how the strategy presenting the best results is the one with some vegetation, whereas the materials with a high albedo improve the microclimate if applied on surfaces characterized by a high sky view factor.},
author = {Salata, Ferdinando and Golasi, Iacopo and Vollaro, Emanuele de Lieto and Bisegna, Fabio and Nardecchia, Fabio and Coppi, Massimo and Gugliermetti, Franco and Vollaro, Andrea de Lieto},
doi = {10.3390/su7079012},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Salata et al. - 2015 - Evaluation of different urban microclimate mitigation strategies through a PMV analysis - Sustainability.pdf:pdf},
isbn = {3906488012},
issn = {20711050},
journal = {Sustainability},
keywords = {ENVI-met,Mitigation strategies,Outdoor thermal comfort,PMV,Urban microclimate},
number = {7},
pages = {9012--9030},
title = {{Evaluation of different urban microclimate mitigation strategies through a PMV analysis}},
volume = {7},
year = {2015}
}
@article{Fiala2012,
abstract = {The UTCI-Fiala mathematical model of human temperature regulation forms the basis of the new Universal Thermal Climate Index (UTC). Following extensive validation tests, adaptations and extensions, such as the inclusion of an adaptive clothing model, the model was used to predict human temperature and regulatory responses for combinations of the prevailing outdoor climate conditions. This paper provides an overview of the underlying algorithms and methods that constitute the multi-node dynamic UTCI-Fiala model of human thermal physiology and comfort. Treated topics include modelling heat and mass transfer within the body, numerical techniques, modelling environmental heat exchanges, thermoregulatory reactions of the central nervous system, and perceptual responses. Other contributions of this special issue describe the validation of the UTCI-Fiala model against measured data and the development of the adaptive clothing model for outdoor climates.},
author = {Fiala, Dusan and Havenith, George and Br{\"{o}}de, Peter and Kampmann, Bernhard and Jendritzky, Gerd},
doi = {10.1007/s00484-011-0424-7},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Fiala et al. - 2012 - UTCI-Fiala multi-node model of human heat transfer and temperature regulation - International Journal of Biometeor.pdf:pdf},
isbn = {1432-1254 (Electronic)$\backslash$r0020-7128 (Linking)},
issn = {00207128},
journal = {International Journal of Biometeorology},
keywords = {Human exposure,Multi-segmental model,Outdoor environment,Physiological simulation,Thermoregulatory system},
number = {3},
pages = {429--441},
