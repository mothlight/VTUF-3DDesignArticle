@article{RademacherI2001,
abstract = {Longitudinal elongation contributes most to leaf area expansion of grasses and its rate is known to be strongly affected by N. Our objective was to determine the effect of two N regimes (N-0 and N+) on the gradient of leaf tissue formation in meristems of two contrasting tall fescue (Festuca arundinacea Schreb.) genotypes. Proportions of epidermal, mesophyll and vascular tissue as well as intercellular air space were determined throughout the base of actively elongating leaves. The area of leaf transverse sections nearly doubled between the ligule and the distal end of the growth zone (about 30 min), and was mainly associated with lateral epidermal and mesophyll cell division in the proximal 5.0-7.5 min. Further increase in transverse area was due to the formation of intercellular airspace and transverse expansion of epidermal cells. Depending on genotype and N treatment the mesophyll, epidermis, vascular bundles and air space comprised 45-54 {\%}, 20-28 {\%}, 6-9 {\%}, and 17-21 {\%}, respectively, of transverse leaf area ill the distal part of the growth zone. After a slight increase close to the leaf base, the area of vascular tissue remained constant throughout the growth zone. The proportion of air space to mesophyll space was higher at N-0 than at N+ because mesophyll area was enhanced by N+ to a greater degree than by N,. In the genotype with slow leaf elongation, the increase in cross-sectional leaf area was due to an increase in both leaf width and leaf thickness, In the genotype which had faster leaf elongation and wider leaves, only leaf thickness was enhanced by N+.},
author = {Rademacher, I. F. and Nelson, C.J.},
doi = {10.1006/anbo.2001.1527},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Rademacher, I. F., Nelson - 2001 - Nitrogen Effects on Leaf Anatomy within the Intercalary Meristems of Tall Fescue Leaf Blades - Annals.pdf:pdf},
isbn = {0305-7364},
issn = {03057364},
journal = {Annals of Botany},
keywords = {d u c t,festuca arundinacea,for,growth zone,i n t ro,i o n,leaf anatomy,nitrogen,of photosynthesis and,organization of the leaf,schreb,tall fescue,the importance of anatomical,the main physiological functions},
number = {5},
pages = {893--903},
title = {{Nitrogen Effects on Leaf Anatomy within the Intercalary Meristems of Tall Fescue Leaf Blades}},
url = {http://aob.oxfordjournals.org/cgi/doi/10.1006/anbo.2001.1527},
volume = {88},
year = {2001}
}
