doi = {10.1175/JAM2462.1},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Coutts, Beringer, Tapper - 2007 - Impact of Increasing Urban Density on Local Climate Spatial and Temporal Variations in the Surface Energy Balance in Melbourne, Australia - Journal of Applied Meteorology and Climatology.pdf:pdf},
isbn = {1558-8424},
