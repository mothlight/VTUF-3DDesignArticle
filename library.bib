keywords = {COMPARATIVE studies,HUMIDITY,SEOUL (Korea),SKIN temperature,Skin temperature,Thermal comfort,Thermal sensation,URBAN forestry,Urban forest},
pages = {133--148},
publisher = {Elsevier GmbH.},
title = {{Comparison of human thermal responses between the urban forest area and the central building district in Seoul, Korea.}},
url = {http://10.0.3.248/j.ufug.2015.12.005{\%}5Cnhttp://search.ebscohost.com/login.aspx?direct=true{\&}db=8gh{\&}AN=113403090{\&}site=ehost-live},
