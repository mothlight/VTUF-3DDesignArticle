issn = {17433509},
journal = {WIT Transactions on the Built Environment},
keywords = {LRT,Pedestrian access,Street connectivity,Transportation,Urban structure,Walkability,Walking},
pages = {305--318},
title = {{Measuring walkability and its effect on light rail usage: A comparative study of the USA and Japan}},
volume = {138},
year = {2014}
}
