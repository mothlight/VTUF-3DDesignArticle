issn = {00068314},
journal = {Boundary-Layer Meteorology},
keywords = {averages,buoyancy effect,effective volume function,improved k,model,spatial and ensemble,urban canopy,urban climate},
number = {3},
pages = {459--490},
title = {{A k-e turbulence closure model for the atmospheric boundary layer including urban canopy}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0036498827{\&}partnerID=40{\&}md5=7793c3027e931309eff66a418431d1d0{\%}5Cnhttp://link.springer.com/article/10.1023/A:1013878907309},
