journal = {Applied Energy},
keywords = {Albedo,Canyon street,Energy performance,Green roofs,Heat and mass transfer},
pages = {273--282},
publisher = {Elsevier Ltd},
title = {{Experimental and numerical investigation of urban street canyons to evaluate the impact of green roof inside and outside buildings}},
url = {http://dx.doi.org/10.1016/j.apenergy.2013.09.073},
volume = {114},
year = {2014}
