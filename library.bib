@article{Cerin2008,
abstract = {This studyexaminedvalidity evidence for the Australian version of theNeighborhood Environment Walkability Scale(NEWS-AU).Astratified two-stage cluster sampling design was used to recruit 2,650 adults from Adelaide (Australia). The sample was drawn from residential addresses within eight high-walkable and eight low- walkable suburbs matched for socio-economic status (SES). Neighborhood walka- bility was measured using Geographic Information Systems data on dwelling density, intersection density, net retail area, and land-use mix. Participants completed the NEWS-AU and reported weekly minutes of walking for transport and recreation (International Physical Activity Questionnaire [IPAQ]). Multilevel confirmatory factor analysis (MCFA) was used to define the individual- and Census Collection District (CCD)-level measurement model of the NEWS-AU. Seven individual- level and five CCD-level factors were identified. These measurement models were somewhat similar to those of the original Neighborhood Environment Walkability Scale (NEWS). Patterns of associations between the NEWS-AU factors/scales and the walking measures provided some validity evidence for the instrument.},
author = {Cerin, Ester and Leslie, Eva and Owen, Neville and Bauman, Adrian},
doi = {10.1080/10913670701715190},
file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Cerin et al. - 2008 - An Australian Version of the Neighborhood Environment Walkability Scale Validity Evidence - Measurement in Physica.pdf:pdf},
isbn = {1091-367X},
issn = {1091-367X},
