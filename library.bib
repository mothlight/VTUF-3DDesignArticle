file = {:home/kerryn/Documents/Work/PhDResearch/GeneralArticles/Brousse et al. - 2016 - WUDAPT, an efficient land use producing data tool for mesoscale models Integration of urban LCZ in WRF over Madr.pdf:pdf},
isbn = {9788578110796},
issn = {22120955},
journal = {Urban Climate},
keywords = {Land use database,Local climate zones,Urban heat island,Urban planning,WRF BEP-BEM,WUDAPT},
pages = {116--134},
pmid = {25246403},
publisher = {Elsevier Inc.},
title = {{WUDAPT, an efficient land use producing data tool for mesoscale models? Integration of urban LCZ in WRF over Madrid}},
url = {http://dx.doi.org/10.1016/j.uclim.2016.04.001},
volume = {17},
year = {2016}
}
