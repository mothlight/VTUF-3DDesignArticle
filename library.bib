keywords = {Buses,Geographical information system,Public transportation,Rapid transit,Road intersection,Walkways},
number = {Mta},
pages = {1--13},
title = {{Measuring Walk Access to Transit in Terms of Sidewalk Availability , Quality , and Connectivity}},
volume = {142},
year = {2015}
