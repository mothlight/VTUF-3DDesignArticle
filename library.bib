title = {{On the Assessment of Surface Heat Flux and Evaporation Using Large-Scale Parameters}},
url = {http://journals.ametsoc.org/doi/abs/10.1175/1520-0493(1972)100{\%}3C0081:OTAOSH{\%}3E2.3.CO;2},
volume = {100},
year = {1972}
